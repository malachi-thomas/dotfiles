[
    // arrow key
    { "key": "cmd+up", "command": "editor.action.insertCursorAbove", "when": "editorTextFocus" },
    { "key": "cmd+down", "command": "editor.action.insertCursorBelow", "when": "editorTextFocus" },
    { "key": "cmd+alt+up", "command": "workbench.action.moveEditorToAboveGroup" },
    { "key": "cmd+alt+down", "command": "workbench.action.moveEditorToBelowGroup" },
    { "key": "cmd+alt+left", "command": "workbench.action.moveEditorToLeftGroup" },
    { "key": "cmd+alt+right", "command": "workbench.action.moveEditorToRightGroup" },
    { "key": "alt+up", "command": "workbench.action.focusAboveGroup" },
    { "key": "alt+down", "command": "workbench.action.focusBelowGroup" },
    { "key": "alt+left", "command": "workbench.action.focusLeftGroup" },
    { "key": "alt+right", "command": "workbench.action.focusRightGroup" },
    { "key": "shift+alt+up", "command": "editor.action.moveLinesUpAction", "when": "editorTextFocus && !editorReadonly" },
    { "key": "shift+alt+down", "command": "editor.action.moveLinesDownAction", "when": "editorTextFocus && !editorReadonly" },
    { "key": "shift+alt+left", "command": "editor.action.moveCarretLeftAction" },
    { "key": "shift+alt+right", "command": "editor.action.moveCarretRightAction" },

    // panles
    { "key": "cmd+shift+x", "command": "workbench.view.extensions" },
    { "key": "cmd+shift+x", "command": "workbench.action.closeSidebar", "when": "sideBarFocus && activeViewlet == 'workbench.view.extensions'" },
    { "key": "cmd+shift+s", "command": "workbench.view.search" },
    { "key": "cmd+shift+s", "command": "workbench.action.closeSidebar", "when": "searchViewletVisible" },
    { "key": "cmd+shift+r", "command": "workbench.action.replaceInFiles" },
    { "key": "cmd+shift+r", "command": "workbench.action.closeSidebar", "when": "searchViewletVisible" },
    { "key": "cmd+shift+f", "command": "workbench.view.explorer" },
    { "key": "cmd+shift+f", "command": "workbench.action.closeSidebar", "when": "sideBarFocus && activeViewlet == 'workbench.view.explorer'" },
    { "key": "cmd+n", "command": "explorer.newFile", "when": "sideBarFocus && activeViewlet == 'workbench.view.explorer'" },
    { "key": "backspace", "command": "deleteFile", "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceReadonly && !inputFocus" },
    { "key": "cmd+d", "command": "explorer.newFolder", "when": "explorerViewletVisible && filesExplorerFocus"},

    // other
    { "key": "cmd+t", "command": "workbench.action.terminal.toggleTerminal", "when": "terminal.active" },
    { "key": "cmd+n", "command": "editor.action.addSelectionToNextFindMatch", "when": "editorFocus" },
    { "key": "cmd+tab", "command": "workbench.action.nextEditor" },
    { "key": "cmd+shift+tab", "command": "workbench.action.previousEditor" },
    { "key": "cmd+shift+n", "command": "editor.action.addSelectionToPreviousFindMatch" },
    { "key": "cmd+r", "command": "editor.action.startFindReplaceAction", "when": "editorFocus || editorIsOpen" },
    { "key": "cmd+h", "command": "editor.action.showHover", "when": "editorTextFocus" },
    { "key": "cmd+/", "command": "editor.action.commentLine", "when": "editorTextFocus && !editorReadonly" },
    { "key": "cmd+shift+a", "command": "editor.action.selectHighlights", "when": "editorFocus" },
    { "key": "cmd+a", "command": "editor.action.replaceAll", "when": "editorFocus && findWidgetVisible" },
    { "key": "cmd+alt+enter", "command": "-editor.action.replaceAll", "when": "editorFocus && findWidgetVisible" },
    { "key": "alt+r", "command": "workbench.action.openRecent" },
    { "key": "cmd+r", "command": "-workbench.action.openRecent" },
    { "key": "cmd+l", "command": "editor.action.reindentlines" },
    { "key": "cmd+[", "command": "editor.fold", "when": "editorTextFocus && foldingEnabled" },
    { "key": "cmd+]", "command": "editor.unfold", "when": "editorTextFocus && foldingEnabled" },
    { "key": "cmd+d", "command": "editor.action.revealDefinition", "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor" },
    { "key": "cmd+shift+n", "command": "jumpToPrevSnippetPlaceholder", "when": "editorTextFocus && hasPrevTabstop && inSnippetMode" },

    // unmap
    { "key": "cmd+d", "command": "-editor.action.addSelectionToNextFindMatch", "when": "editorFocus"},
    { "key": "cmd+n", "command": "-workbench.action.files.newUntitledFile"},
    { "key": "cmd+r", "command": "renameFile", "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus" },
    { "key": "cmd+n", "command": "jumpToNextSnippetPlaceholder", "when": "editorTextFocus && hasNextTabstop && inSnippetMode" },
    { "key": "tab", "command": "-jumpToNextSnippetPlaceholder", "when": "editorTextFocus && hasNextTabstop && inSnippetMode" },

]